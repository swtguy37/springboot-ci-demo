import java.util.Arrays;
import java.util.List;
import java.util.stream.Collectors;
import java.util.stream.Stream;

public class Extract {

	public static void main(String[] args) {
		doStuff();
	}

	private static void doStuff() {
		// TODO Auto-generated method stub
		String stMn = "a12b342c4576d";
		
		char[] strVals =  stMn.toCharArray();
		
//		Arrays.asList(strVals).stream().filter(x->Character.isDigit((char)x);
		
		stMn = stMn.replaceAll("\\D", "");
		
		System.out.println("vals " + stMn);
		
		
		List<Integer> collect = stMn.chars().filter(Character::isDigit).mapToObj(x->Character.getNumericValue(x)).collect(Collectors.toList());
		
	}
}
